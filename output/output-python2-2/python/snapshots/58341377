class GUImainWindow(myQMainWindow):
    def __init__(self,parent = None,name = None,fl = 0):
        QMainWindow.__init__(self,parent,name,fl)
        #self.statusBar()

        self.image0 = QPixmap()
        self.image0.loadFromData(image0_data,"PNG")
        self.image1 = QPixmap()
        self.image1.loadFromData(image1_data,"PNG")
        self.image2 = QPixmap()
        self.image2.loadFromData(image2_data,"PNG")
        self.image3 = QPixmap()
        self.image3.loadFromData(image3_data,"PNG")
        self.image4 = QPixmap(image4_data)
        self.image5 = QPixmap(image5_data)
        self.image6 = QPixmap()
        self.image6.loadFromData(image6_data,"PNG")
        self.image7 = QPixmap()
        self.image7.loadFromData(image7_data,"PNG")       
	self.image8 = QPixmap(image8_data)
	self.imageComit = QPixmap()
        self.imageComit.loadFromData(imageComit_data,"PNG")
	self.imageConnectMode = QPixmap()
        self.imageConnectMode.loadFromData(imageConnectMode_data,"PNG")


        if not name:
            self.setName("mainWindow")

	self.central=QWidget(self)
        self.central.setMinimumSize(QSize(800,600))
	self.setCentralWidget(self.central)


	## Set Actions, toggled ? with icon ?
        self.fileExitAction = QAction(self,"fileExitAction")
        self.helpAboutAction = QAction(self,"helpAboutAction")
        self.aMassInsert = QAction(self,"fileMass_insert_data_from_fileAction")
        self.fileStore_object_in_DBAction = QAction(self,"fileStore_object_in_DBAction")
	self.fileStore_object_in_DBAction.setIconSet(QIconSet(self.imageComit))
        self.aGenerateSQL = QAction(self,"fileGenerate_ConfDBLibrary_fileAction")
        self.aConnect = QAction(self,"aConnect")
        #self.aConnect.setToggleAction(1)
        self.aConnect.setIconSet(QIconSet(self.image4))
        self.aDisconn = QAction(self,"aDisconn")
        #self.aDisconn.setToggleAction(1)
        self.aDisconn.setEnabled(0)
        self.aDisconn.setIconSet(QIconSet(self.image5))
        self.aZoomIn = QAction(self,"aZoomIn")
        self.aZoomIn.setIconSet(QIconSet(self.image6))
        self.aZoomOut = QAction(self,"aZoomOut")
        self.aZoomOut.setIconSet(QIconSet(self.image7))        
	self.aOnPathView = QAction(self,"OnPathView")
        self.aOnPathView.setIconSet(QIconSet(self.image8))
	self.aOnPathView.setToggleAction(1)
	self.aModify = QAction(self,"Action Modify")
	self.aDelete = QAction(self,"Action Delete")
	self.aCreateDeviceType = QAction(self,"Action Create Device")
	self.aCreateDevices = QAction(self,"Action Create Devices")
	self.aKeep = QAction(self,"Keep on canvas")
	self.aCreateLinkMode = QAction(self,"Create connexion")
        self.aCreateLinkMode.setToggleAction(1)
	self.aCreateLinkMode.setIconSet(QIconSet(self.imageConnectMode))
	self.aCreateLinkMode.setText("Create Link")
	self.aCreateLinkMode.setMenuText("Create Link")

	
	## Adding Actions to ToolBar
        self.Toolbar = QToolBar(QString(""),self,Qt.DockTop)

        self.aConnect.addTo(self.Toolbar)
        self.aDisconn.addTo(self.Toolbar)
        self.aZoomIn.addTo(self.Toolbar)
        self.aZoomOut.addTo(self.Toolbar)
        self.aOnPathView.addTo(self.Toolbar)
        self.aCreateLinkMode.addTo(self.Toolbar)
        self.fileStore_object_in_DBAction.addTo(self.Toolbar)


        self.MenuBar = QMenuBar(self,"MenuBar")

	## Adding actions to different Menus
        self.fileMenu = QPopupMenu(self)
        self.aConnect.addTo(self.fileMenu)
        self.aDisconn.addTo(self.fileMenu)
        self.fileMenu.insertSeparator()
        self.aMassInsert.addTo(self.fileMenu)
        self.fileMenu.insertSeparator()
        self.fileStore_object_in_DBAction.addTo(self.fileMenu)
        self.aGenerateSQL.addTo(self.fileMenu)
        self.fileMenu.insertSeparator()
        self.fileExitAction.addTo(self.fileMenu)
        self.MenuBar.insertItem(QString("File"),self.fileMenu,1)

        self.Edit = QPopupMenu(self)
        self.aKeep.addTo(self.Edit)
	self.aModify.addTo(self.Edit)
	self.aDelete.addTo(self.Edit)
	self.MenuBar.insertItem(QString("Edit"),self.Edit,2)

        self.Create = QPopupMenu(self)
	self.aCreateDeviceType.addTo(self.Create)
	self.aCreateDevices.addTo(self.Create)
        self.MenuBar.insertItem(QString("Create"),self.Create,3)

        self.helpMenu = QPopupMenu(self)
        self.helpAboutAction.addTo(self.helpMenu)
        self.MenuBar.insertItem(QString("Help"),self.helpMenu,4)

	## GENERAL LAYOUT 
        myLayout = QGridLayout(self.centralWidget(),1,1,0,0,"MyLayout")

        self.panel = mainPanel(self,self.centralWidget(),"panelFrame")
        self.panel.setFrameShape(QFrame.StyledPanel)
        self.panel.setFrameShadow(QFrame.Raised)
        myLayout.addWidget(self.panel,0,0)

        self.myStatusbar = myStatusBar(self.centralWidget(),"mystattusbarrr")
        myLayout.addWidget(self.myStatusbar,1,0)
	
	self.resize(QSize(800,600).expandedTo(self.minimumSizeHint()))
	
	
	## StatusBar
	# self.statusbarMsg = QLabel("MESSAGE ",self)
	# self.statusbarMsg.resize(400,20)
	# self.statusBar().addWidget(self.statusbarMsg)	
	# self.statusbarView = QLabel("100%",self)
	# self.statusBar().addWidget(self.statusbarView) #self.statusBar().addWidget(self.statusbarView)
	# self.statusbarZoom = QLabel("MESSAGE ",self)
	# self.statusBar().addWidget(self.statusbarZoom)
	# self.statusbarMode = QLabel("-- ",self)
	# self.statusBar().addWidget(self.statusbarMode)
	# self.statusbarMode2 = QLabel("MESSAGE ",self)
	# self.statusBar().addWidget(self.statusbarMode2)
	
        self.languageChange()

        self.resize(QSize(800,600).expandedTo(self.minimumSizeHint()))
        self.clearWState(Qt.WState_Polished)

        self.connect(self.aConnect,SIGNAL("activated()"),self.OnConnect)
        self.connect(self.aDisconn,SIGNAL("activated()"),self.OnDisconnect)
        self.connect(self.aZoomIn,SIGNAL("activated()"),self.ZoomIn)
        self.connect(self.aZoomOut,SIGNAL("activated()"),self.ZoomOut)
        self.connect(self.aOnPathView,SIGNAL("toggled(bool)"),self.OnPathView)
	self.connect(self.aMassInsert,SIGNAL("activated()"),self.MassInsert)
	self.connect(self.aGenerateSQL,SIGNAL("activated()"),self.GenerateSQL)
	# self.connect(self.aClone,SIGNAL("activated()"),self.panel.visWindow.DoCloneHelper)
	# self.connect(self.aDuplicate,SIGNAL("activated()"),self.panel.visWindow.DoDuplicateHelper)
	self.connect(self.aModify,SIGNAL("activated()"),self.DoModify)
	self.connect(self.aDelete,SIGNAL("activated()"),self.DoDelete)
	self.connect(self.aCreateDeviceType,SIGNAL("activated()"),self.CreateDeviceTypeClick)
	self.connect(self.aCreateDevices,SIGNAL("activated()"),self.CreateDeviceClick)
	# self.connect(self.aCreateLinkType,SIGNAL("activated()"),self.CreateLinkTypeClick)
	# self.connect(self.aCreateLink,SIGNAL("activated()"),self.CreateLinkClick)
	self.connect(self.aKeep,SIGNAL("activated()"),self.panel.visWindow.keep)
	self.connect(self.fileStore_object_in_DBAction,SIGNAL("activated()"),self.OnSave)
	##self.connect(self.aCreateLinkMode,SIGNAL("toggled(bool)"),self.SetCreationMode)
	self.connect(self.helpAboutAction,SIGNAL("activated()"),self.About)

    def languageChange(self):
        self.setCaption(self.__tr("CdbVis - Configuration Database Visualizer"))
        '''self.fileNewAction.setText(self.__tr("New"))
        self.fileNewAction.setMenuText(self.__tr("&New"))
        self.fileNewAction.setAccel(self.__tr("Ctrl+N"))
        self.fileOpenAction.setText(self.__tr("Open"))
        self.fileOpenAction.setMenuText(self.__tr("&Open..."))
        self.fileOpenAction.setAccel(self.__tr("Ctrl+O"))
        self.fileSaveAction.setText(self.__tr("Save"))
        self.fileSaveAction.setMenuText(self.__tr("&Save"))
        self.fileSaveAction.setAccel(self.__tr("Ctrl+S"))
        self.fileSaveAsAction.setText(self.__tr("dsds"))
        self.fileSaveAsAction.setMenuText(self.__tr("dsds"))
        self.fileSaveAsAction.setAccel(QString.null)
        self.filePrintAction.setText(self.__tr("Print"))
        self.filePrintAction.setMenuText(self.__tr("&Print..."))
        self.filePrintAction.setAccel(self.__tr("Ctrl+P"))'''
        self.fileExitAction.setText(self.__tr("Exit"))
        self.fileExitAction.setMenuText(self.__tr("Exit"))
        self.helpAboutAction.setText(self.__tr("About"))
        self.helpAboutAction.setMenuText(self.__tr("&About"))
        self.helpAboutAction.setAccel(QString.null)
        self.aDisconn.setText(self.__tr("Disconnect"))
        self.aDisconn.setMenuText(self.__tr("Disconnect"))
        self.aMassInsert.setText(self.__tr("Mass insert data from file"))
        self.aMassInsert.setMenuText(self.__tr("Mass insert data from file"))
        self.fileStore_object_in_DBAction.setText(self.__tr("Store object in ConfDB"))
        self.fileStore_object_in_DBAction.setMenuText(self.__tr("Store object in ConfDB"))
        self.aGenerateSQL.setText(self.__tr("Generate ConfDBLibrary function file"))
        self.aGenerateSQL.setMenuText(self.__tr("Generate ConfDBLibrary function file"))
        self.aConnect.setText(self.__tr("Connect"))
	self.aZoomIn.setText(self.__tr("Zoom in"))
	self.aZoomOut.setText(self.__tr("Zoom out"))
	self.aModify.setText(self.__tr("Modify"))
	self.aDelete.setText(self.__tr("Delete"))
	self.aCreateDeviceType.setText(self.__tr("Create Device Type"))
	self.aCreateDevices.setText(self.__tr("Create Devices"))
	self.aKeep.setText(self.__tr("Keep visible"))
        self.aOnPathView.setText(self.__tr("Paths View"))
        self.aOnPathView.setMenuText(self.__tr("Paths View"))
	

    def About(self):
	QMessageBox.about(self,"About Configuration Database Visualizer","CDBVis\nConfiguration Database Visualizer\nVersion 3.r2\nDate : 31/08/2007\nFelix Schmidt-Eisenlohr, CERN, 2004\nThomas Johansen, CERN, 2005-2006\nWalid Belballi, CERN, 2007\n Supervisor: Eric Van Herwijnen\nWeb Page: http://lhcb-online.web.cern.ch/lhcb-online/configurationdb/cdbVis.htm") 

    def OnDisconn(self):
        print "mainWindow.OnDisconn(): Not implemented yet"

    def __tr(self,s,c = None):
        return qApp.translate("mainWindow",s,c)
