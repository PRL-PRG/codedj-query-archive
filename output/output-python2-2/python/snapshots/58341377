class myStatusBar(QFrame):
    def __init__(self,parent,name="My StatusBar"):
	QFrame.__init__(self,parent,name)

	self.imageInfo = QPixmap()
	self.imageInfo.loadFromData(imageInfo_data,"PNG")  
	self.imageWarning = QPixmap()
	self.imageWarning.loadFromData(imageWarning_data,"PNG") 
	self.imageCheck = QPixmap()
	self.imageCheck.loadFromData(imageCheck_data,"PNG") 
	self.imageCross = QPixmap()
	self.imageCross.loadFromData(imageCross_data,"PNG") 
	self.imageClock = QPixmap()
	self.imageClock.loadFromData(imageClock_data,"PNG") 
	self.imageZoom = QPixmap()
	self.imageZoom.loadFromData(imageZoom_data,"PNG") 
	self.imagePlus = QPixmap()
	self.imagePlus.loadFromData(imagePlus_data,"PNG")
        self.imageNavigation = QPixmap()
        self.imageNavigation.loadFromData(imageNavigation_data,"PNG")
        self.imageCreation = QPixmap()
        self.imageCreation.loadFromData(imageCreation_data,"PNG")	

	GridLayout = QGridLayout(self,1,5,0,0,"Layout")
        self.icon = QLabel(self,"Info type")
	self.icon.setPixmap(self.imageInfo)
	self.icon.setMaximumSize(QSize(30,20))
        GridLayout.addWidget(self.icon,0,0)
        
	self.info = QLineEdit(self,"Informations")
        self.info.setPaletteBackgroundColor(QColor(224,223,227))
        self.info.setFocusPolicy(QLineEdit.NoFocus)
        self.info.setLineWidth(0)
        self.info.setReadOnly(1)
        GridLayout.addWidget(self.info,0,1)
        self.zoomIcon = QLabel(self)
	self.zoomIcon.setPixmap(self.imageZoom) #self.label2.setMaximumSize(QSize(100,20))
        GridLayout.addWidget(self.zoomIcon,0,2)
        self.zoom = QLabel("100%",self,"zoom")
	self.zoom.setFixedWidth(100) #self.label2.setMaximumSize(QSize(100,20))
        GridLayout.addWidget(self.zoom,0,3)
        self.modeIcon = QLabel(self)
	self.modeIcon.setPixmap(self.imageNavigation) #self.label2.setMaximumSize(QSize(100,20))
        GridLayout.addWidget(self.modeIcon,0,4)
        self.mode = QLabel("Navigation Mode",self,"label1")
	self.mode.setFixedWidth(100) #self.label3.setMaximumSize(QSize(100,20))
        GridLayout.addWidget(self.mode,0,5)

	self.setLineWidth(0)
        self.setMinimumSize(QSize(0,20))
        self.setMaximumSize(QSize(32767,20))
        self.setFrameShape(QFrame.StyledPanel)
	self.setFrameShadow(QFrame.Raised)

	
