
class CDataType:
    
    def __init__(self, id):
        self.id = id
        self.dataTypes = []
    
    def AddDataType(self, type):
        self.dataTypes.append(type)
    
    def GetDataTypes(self):
        for i in self.dataTypes:
            yield i
    
    def DeleteDataType(self, type):
        self.remove(type)