
class CLanguageType:
    def __init__(self, id):
        self.id = id
        self.icon = None
    
    
    def GetId(self):
        return self.id
    
    def GetIcon(self):
        return self.icon
    
    def SetId(self, id):
        self.id = id
    
    def SetIcon(self, icon):
        self.icon = icon
        