## just for local tests:
import os

afss = []
wins = []
niks = []

afsdir = '/afs/cern.ch/user/i/ibelyaev/vol10/DATA/'
windir = 'C:\\data\\JpsiPhi\\'
nikdir = '/data/bfys/vanya/JpsiPhi/'

for f in LFNs_ :
	
        i = f.rfind("/")
        if -1 == i : continue
        afs = afsdir + f[i+1:]
        win = windir + f[i+1:]
	nik = nikdir + f[i+1:]
        if   os.path.exists ( win ) : wins += [ win ]
        elif os.path.exists ( nik ) : niks += [ nik ]
        elif os.path.exists ( afs ) : afss += [ afs ]

if wins :

        FILEs = wins
        print "Use local WIN-files #%s at '%s'"%(len(FILEs),windir)
	
elif niks :

        FILEs = niks
        print "Use local files #%s at '%s'"%(len(FILEs),nikdir)
	
elif afss :
	
        FILEs = afss
        print "Use local AFS-files #%s at '%s'"%(len(FILEs),afsdir)
        
else :
        print "Use local LFN-files #%s "%len(FILEs)
	if catalog :
		from GaudiPython import AppMgr 
		gaudi = AppMgr() 
		opts =  "FileCatalog.Catalog = %s "% catalog_CERN  
		gaudi.config ( options = [ opts ] ) 
		
