class CVisualObject:
    def __init__(self):
        self.parent = None
    
    def GetHeight(self, element):
        return 0

    def GetWidth(self, element):
        return 0

    def GetParent(self):
        return self.parent

    def Paint(self, x, y, element, w = None, h = None):
        pass
    
    def PaintShadow(self, x, y, element, color, w = None, h = None):
        pass

    def SetParent(self, parent):
        self.parent = parent
