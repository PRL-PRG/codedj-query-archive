import pygtk
pygtk.require( '2.0' )
import gtk

class SubActivity(gtk.EventBox):
    
    def __init__(self, set_mode):
        gtk.EventBox.__init__(self)
        self.set_mode = set_mode

    def onDestroy(self):
        pass
    def onKeyPress(self, widget, data):
        pass
    def onKeyRelease(self, widget, data):
        pass 
    def onActivate(self):
        pass
    def onDeactivate(self):
        pass
